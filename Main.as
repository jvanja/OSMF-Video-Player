package{	import flash.display.Sprite;	import flash.events.MouseEvent;	import org.osmf.events.TimeEvent;	import org.osmf.media.MediaPlayerSprite;	import org.osmf.media.URLResource;	import com.vanjajelic.UI;	import com.vanjajelic.WebTracker;		public class Main extends Sprite	{		public var mps:MediaPlayerSprite;		private var ui:UI;		private var gaTracker:WebTracker;				public function Main()		{			init();		}				private function init():void		{			mps = new MediaPlayerSprite();			addChild(mps);			mps.resource = new URLResource("demo.flv");						ui = new UI(this);			addChild(ui);						gaTracker=WebTracker.getInstance();			gaTracker.setUpGA(this, "UA-11111-1", false);						mps.mediaPlayer.addEventListener(TimeEvent.CURRENT_TIME_CHANGE, onCurrentTimeChange);		}		private function onCurrentTimeChange(event:TimeEvent):void		{			//trace("TIMER: " + secondsToMinutesSeconds(Math.floor(event.time)));			var perc:Number = event.time / mps.mediaPlayer.duration;			ui.progressBar.progress.scaleX = perc;					}				//////////////////////////////////////////////////////////////////		////// UTILS				private function secondsToMinutesSeconds(secs:Number):String 		{			var s:Number = secs % 60;			var m:Number = Math.floor((secs % 3600 ) /60);			var h:Number = Math.floor(secs / (60*60));						var hourstring:String = h<10 ? ""+h+":" : h+":";			if (h==0) hourstring="";			var minutestring:String = m<10 ? "0"+m+":" : m+":";			var secstring:String = s<10 ? "0"+s : String(s);			return hourstring+minutestring+secstring;		}	}}